<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:FlyoutPageSample"
            xmlns:knxSecureBusmonitorMaui="clr-namespace:KNX_Secure_Busmonitor_MAUI"
            x:Class="FlyoutPageSample.AppFlyout">
    <FlyoutPage.Flyout>
        <knxSecureBusmonitorMaui:FlyoutMenuPage x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <knxSecureBusmonitorMaui:MainPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>